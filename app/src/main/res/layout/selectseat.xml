<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="412dp"
        android:layout_height="43dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#D9D9D9"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="0dp" />


    <ImageView
        android:id="@+id/vector1"
        android:layout_width="17dp"
        android:layout_height="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/vector1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="13dp" />



    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Origin"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="81dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Destination"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="263dp"
        tools:layout_editor_absoluteY="81dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Select Seat"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="16dp" />


    <ImageView
        android:id="@+id/vector_1"
        android:layout_width="110dp"
        android:layout_height="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/vector2"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="91dp" />


    <TextView
        android:id="@+id/depart_time"
        android:layout_width="74dp"
        android:layout_height="23dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="top"
        android:text="Depart: time"
        android:textColor="#A2A2A2"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="117dp" />

    <TextView
        android:id="@+id/arrive_time"
        android:layout_width="67dp"
        android:layout_height="24dp"
        app:layout_constraintStart_toEndOf="@+id/depart_time"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="top"
        android:text="Arrive: time"
        android:textColor="#A2A2A2"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="283dp"
        tools:layout_editor_absoluteY="117dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Duration"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/price"
        android:layout_width="47dp"
        android:layout_height="23dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="top"
        android:text="Price"
        android:textColor="#000000"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/feb_sun"
        android:layout_width="91dp"
        android:layout_height="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="top"
        android:text="1 Feb, Sun"
        android:textColor="#A2A2A2"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="164dp" />

</androidx.constraintlayout.widget.ConstraintLayout>