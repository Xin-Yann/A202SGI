<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="15dp">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#040F51"
            android:gravity="center"
            android:text="Booking History"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="170dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="17dp"
            android:layout_height="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="-35dp"
            app:srcCompat="@drawable/left_arrow_white" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="-5dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/rectangle_41" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="140dp"
            android:layout_height="28dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="-33dp"
            app:srcCompat="@drawable/rectangle_42" />

        <TextView
            android:id="@+id/upcoming_trips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="-25dp"
            android:gravity="top"
            android:text="Upcoming Trips"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/history"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="255dp"
            android:layout_marginTop="-20dp"
            android:gravity="top"
            android:text="History"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/rectangle_40"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            app:srcCompat="@drawable/rectangle_40" />

        <TextView
            android:id="@+id/train"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="-115dp"
            android:gravity="top"
            android:text="Train"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/completed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="298dp"
            android:layout_marginTop="-21dp"
            android:gravity="top"
            android:text="Completed"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/depart_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="10dp"
            android:gravity="top"
            android:text="Depart Time"
            android:textColor="#000000"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/origin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="68dp"
            android:layout_marginTop="5dp"
            android:gravity="top"
            android:text="Origin"
            android:textColor="#A2A2A2"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/duration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-58dp"
            android:gravity="top"
            android:text="Duration"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="80dp"
            android:layout_height="12dp"
            android:layout_marginStart="118dp"
            android:layout_marginTop="-2dp"
            app:srcCompat="@drawable/new_moon_blue" />

        <TextView
            android:id="@+id/line"
            android:layout_width="90dp"
            android:layout_height="15dp"
            android:layout_marginStart="162dp"
            android:layout_marginTop="-17dp"
            android:gravity="top"
            android:text="---------------------------"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="80dp"
            android:layout_height="12dp"
            android:layout_marginStart="215dp"
            android:layout_marginTop="-11dp"
            app:srcCompat="@drawable/dry_clean" />

        <TextView
            android:id="@+id/arrival_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="275dp"
            android:layout_marginTop="-24dp"
            android:gravity="top"
            android:text="Arrival Time"
            android:textColor="#000000"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/destination"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="285dp"
            android:layout_marginTop="5dp"
            android:gravity="top"
            android:text="Destination"
            android:textColor="#A2A2A2"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/ticket_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="10dp"
            android:gravity="top"
            android:text="Ticket Type"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/seats"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="325dp"
            android:layout_marginTop="-20dp"
            android:gravity="top"
            android:text="Seats"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/rectangle_40two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            app:srcCompat="@drawable/rectangle_40" />

        <TextView
            android:id="@+id/train2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="-115dp"
            android:gravity="top"
            android:text="Train"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/completed2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="298dp"
            android:layout_marginTop="-21dp"
            android:gravity="top"
            android:text="Completed"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/depart_time2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="10dp"
            android:gravity="top"
            android:text="Depart Time"
            android:textColor="#000000"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/origin2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="68dp"
            android:layout_marginTop="5dp"
            android:gravity="top"
            android:text="Origin"
            android:textColor="#A2A2A2"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/duration2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-58dp"
            android:gravity="top"
            android:text="Duration"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/new_moon_blue2"
            android:layout_width="80dp"
            android:layout_height="12dp"
            android:layout_marginStart="118dp"
            android:layout_marginTop="-2dp"
            app:srcCompat="@drawable/new_moon_blue" />

        <TextView
            android:id="@+id/line2"
            android:layout_width="90dp"
            android:layout_height="15dp"
            android:layout_marginStart="162dp"
            android:layout_marginTop="-17dp"
            android:gravity="top"
            android:text="---------------------------"
            android:textColor="#000000"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/dry_clean2"
            android:layout_width="80dp"
            android:layout_height="12dp"
            android:layout_marginStart="215dp"
            android:layout_marginTop="-11dp"
            app:srcCompat="@drawable/dry_clean" />

        <TextView
            android:id="@+id/arrival_time2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="275dp"
            android:layout_marginTop="-24dp"
            android:gravity="top"
            android:text="Arrival Time"
            android:textColor="#000000"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/destination2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="285dp"
            android:layout_marginTop="5dp"
            android:gravity="top"
            android:text="Destination"
            android:textColor="#A2A2A2"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/ticket_type2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="10dp"
            android:gravity="top"
            android:text="Ticket Type"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/seats2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="325dp"
            android:layout_marginTop="-20dp"
            android:gravity="top"
            android:text="Seats"
            android:textColor="#000000"
            android:textSize="14sp" />
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/footer"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#FEFAFD"
        android:gravity="center_horizontal"
        android:elevation="50dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:layout_marginLeft="60dp">

            <ImageButton
                android:id="@+id/image1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffff"
                android:contentDescription="Image1"
                android:onClick="clickImage1"
                android:padding="5dp"
                app:srcCompat="@drawable/home"
                android:layout_marginRight="88dp"/>

            <ImageButton
                android:id="@+id/image2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:onClick="clickImage2"
                app:srcCompat="@drawable/ticket"
                android:layout_marginRight="95dp"
                android:layout_marginLeft="5dp"/>


            <ImageButton
                android:id="@+id/image3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:onClick="clickImage3"
                android:padding="5dp"
                app:srcCompat="@drawable/account" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:padding="10dp"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal">


            <TextView
                android:id="@+id/home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:text="Home"
                android:textColor="#000000"
                android:textSize="14sp"
                android:layout_marginRight="70dp"/>

            <TextView
                android:id="@+id/myTicket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:text="My Ticket"
                android:textColor="#000000"
                android:textSize="14sp"
                android:layout_marginRight="70dp"
                android:layout_marginLeft="10dp"/>

            <TextView
                android:id="@+id/account"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:text="Account"
                android:textColor="#000000"
                android:textSize="14sp" />
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>